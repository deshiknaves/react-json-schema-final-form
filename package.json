{
  "name": "react-json-schema-final-form",
  "version": "0.0.1",
  "description": "Render a Final Form using JSON schema and UI Schema",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "eslint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|md|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "precommit": "lint-staged",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm run test:coverage",
    "storybook": "start-storybook -p 6006",
    "start": "npm run storybook",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.5.3",
    "jest-runner-eslint": "^0.7.7",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "test-data-bot": "^0.8.0"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "keywords": [
    "final",
    "form",
    "react",
    "JSON",
    "schema"
  ],
  "author": "Deshiknaves",
  "license": "ISC"
}
